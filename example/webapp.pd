module main

import (
	"http"
    "log"
)

def main()
    log = log.New()

    app = http.NewApp()
        .Get("/",      (req, resp) => http.Static(req, resp, http.Dir("static")))
        .Get("/hello", (req, resp) => resp.Write("Hello, World!" to [byte]))
    
    log = log.Fatal(app.ListenAndServe(":8080"))
    