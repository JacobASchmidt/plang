module users 

import (
    "uuid"
    "email"
    "phone"
    "db"
)

databaseAddr = "cool"
cradentials = db.FromEnv("db-username", "db-password")

type ID uuid

type User struct
    ID    ID 
    Name  string 
    Email email.Address 
    Phone phone.Number

def FromId(id ID) db.Result[User]
    con = db.Open(databaseAddr, cradentials)?
    return con.Query("Select user from users where user.ID = ?", id)?
